	<queryString>
		<![CDATA[SELECT
     cliente."nome" AS cliente_nome,
     produto."descricao" AS produto_descricao,
     produto."valor" AS produto_valor,
     venda."data_venda" AS venda_data_venda
FROM
     "public"."cliente" cliente INNER JOIN "public"."venda" venda ON cliente."id" = venda."id_cliente"
     INNER JOIN "public"."produto" produto ON venda."id_produto" = produto."id"
WHERE cliente.id=$P{idCliente}]]>
	</queryString>
